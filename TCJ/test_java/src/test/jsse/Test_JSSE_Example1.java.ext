### Create %%jks%% %%Certificate%% by tool %%keytool%% @TAG(jks, keytool)

        keytool -genkey -alias test -keypass 123456 -keyalg RSA -keysize 1024 -validity 365 -keystore test.jks -storepass 123456

        keytool -list -v -keystore test.jks

